<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.alipapa.seat.dao.RecordDao">
    <insert id="userIn" parameterType="cn.alipapa.seat.bean.Record" keyProperty="Record.id" keyColumn="record.id" useGeneratedKeys="true">
    INSERT INTO record (user_id,enter_time,record_date)
    VALUES (#{user_id},#{enterTime},#{recordDate})

        <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
            SELECT LAST_INSERT_ID()
        </selectKey>
    </insert>
</mapper>

